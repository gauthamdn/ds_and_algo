package dynamicprogramming;

public class Subsequence {

	public static void main(String[] args) {
		
		System.out.println(printSubsequence("abc",0," "));
		

	}
	
	
	public static String printSubsequence(String s, int i, String sfos) {
		
		char[] charArray = s.toCharArray();
		
		if(i>=s.length()-1)
			return charArray[i];
		
		
		
		return printSubsequence(s,i+1,);
	}

}
